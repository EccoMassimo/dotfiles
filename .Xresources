!! URxvt 
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.internalBorder: 15 
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 3000 
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.font: xft: DejaVu Sans Mono : size=11
URxvt.boldFont: xft: DejaVu Sans Mono: size=11
!URxvt.font: xft: Unifont : size=12
!URxvt.boldFont: xft: Unifont : size=12


!! Rofi Settings


!! Plain Black and White
rofi.color-enabled: true 
rofi.color-window: #000000, #ffffff, #000000
rofi.color-normal: #000000, #ffffff, #000000, #ffffff, #000000
rofi.color-active: #000000, #ffffff, #000000, #ffffff, #000000
rofi.color-urgent: #000000, #ffffff, #000000, #ffffff, #000000

!! Massimo Colors
!rofi.color-enabled: true 
!rofi.color-window: #101010, #cccccc, #101010
!rofi.color-normal: #101010, #cccccc, #101010, #cccccc, #101010
!rofi.color-active: #101010, #cccccc, #101010, #cccccc, #101010
!rofi.color-urgent: #101010, #cccccc, #101010, #cccccc, #101010

rofi.modi: run,window
rofi.sidebar-mode: true
rofi.terminal: urxvt


